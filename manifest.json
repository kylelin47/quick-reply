{
  "manifest_version": 2,

  "name": "Quick Reply",
  "description": "This extension automatically generates a reply",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Quick Reply"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "clipboardWrite"
  ]
}
